<!DOCTYPE html>
<h1>Players</h1>
<a href='players-new'>Add new player</a>
<br>
<br>
<table id="playerTable" style="width:100%" class="table table-striped">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Position</th>
    <th>Height</th>
    <th>Weight</th>
    <th>Team</th>
    <th></th>
  </tr>
  {{#each results}}
  <tr id={{this.playerID}}>
    <td id="firstName" contenteditable="true" onblur="editRow()">{{this.firstName}}</td>
    <td id="lastName" contenteditable="true" onblur="editRow()">{{this.lastName}}</td>
    <td id="position" contenteditable="true" onblur="editRow()">{{this.position}}</td>
    <td id="height" contenteditable="true" onblur="editRow()">{{this.height}}</td>
    <td id="weight" contenteditable="true" onblur="editRow()">{{this.weight}}</td>
    <td id={{this.teamID}}>{{this.teamName}}</td>
    <td>
      <button>Delete</button>
    </td>
  </tr>
  {{/each}}
</table>
<script src="editTable.js"></script>