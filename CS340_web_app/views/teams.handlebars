<!DOCTYPE html>
<h1>Teams</h1>
<a href='teams-new'>Add new team</a>
<br>
<br>
<table style="width:100%" class="table table-striped">
  <tr>
    <th>Location</th>
    <th>Name</th>
    <th>Year Founded</th>
    <th>Majority Owner</th>
    <th>Conference</th>
    <th>Division</th>
    <th></th>
  </tr>
  {{#each results}}
  <tr id={{this.teamID}}>
    <td>{{this.location}}</td>
    <td>{{this.name}}</td>
    <td>{{this.year_founded}}</td>
    <td>{{this.majority_owner}}</td>
    <td>{{this.conference}}</td>
    <td>{{this.division}}</td>
    <td>
      <input type="button" value="Delete" onclick="deleteRow()">
      <input type="button" value="Edit">
    </td>
  </tr>
  {{/each}}
</table>
<script src="editTable.js"></script>